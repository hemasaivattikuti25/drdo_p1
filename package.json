{
  "name": "favcart",
  "version": "2.0.0",
  "description": "E-commerce with FastAPI Backend and React Frontend",
  "scripts": {
    "server": "cd backend && uvicorn main:app --reload --port 8000",
    "client": "cd frontend && npm start",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd frontend && npm run build",
    "install-frontend": "cd frontend && npm install"
  },
  "keywords": [
    "ecommerce",
    "mongodb",
    "fastapi",
    "react"
  ],
  "author": "Sai",
  "license": "MIT",
  "devDependencies": {
    "concurrently": "^7.6.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}
